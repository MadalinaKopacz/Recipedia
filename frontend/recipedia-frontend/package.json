{
  "name": "recipedia-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "axios": "^1.2.1",
    "axios-cookiejar-support": "^4.0.3",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.5.0",
    "react-scripts": "5.0.1",
    "tough-cookie": "^4.1.2",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "env-cmd -f .env react-scripts build",
    "start:api": "cd ../../backend/recipedia/ && python manage.py migrate && PYTHONUNBUFFERED=1 python manage.py runserver 127.0.0.1:8000 --noreload",
    "start:app": "BROWSER=none react-scripts start | cat",
    "test:start-app": "start-server-and-test --expect 404 start:api http-get://127.0.0.1:8000 start:app",
    "test": "start-server-and-test test:start-app http://127.0.0.1:3000 test:cypress",
    "cloud": "start-server-and-test test:start-app http://127.0.0.1:3000 cloud:cypress",
    "test:react-scripts": "react-scripts test",
    "test:cypress": "cypress run",
    "cloud:cypress": "cypress run --record",
    "eject": "react-scripts eject"
  },
  "proxy": "https://api.edamam.com",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "cypress": "^12.4.0",
    "env-cmd": "^10.1.0",
    "start-server-and-test": "^1.15.3"
  }
}
